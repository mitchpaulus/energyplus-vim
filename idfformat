#!/usr/bin/awk -E

# This script helps reformat an idf file.
BEGIN { isIndent = 0; searchForObject = 1 }

{
    if ($0 ~ /^[ ]*$/) { print ""; next }

    gsub(/^[ \t]*/, "")
    if (isIndent) { print "  " $0 } else { print $0 }

    if (searchForObject) {

        indexForComment = match($0, /!/)

        # Don't want to search in the comments for anything.
        searchableContent = indexForComment > 0 ?
                                substr($0, 1, indexForComment - 1) :
                                $0

        if (isObjectType(searchableContent)) {
            searchForObject = 0
            isIndent = 1
        }
    }

    # If we've reached a terminator line, reset to no indent and search for an object type.
    if ($0 ~ ";") { isIndent = 0; searchForObject = 1 }
}


function isObjectType(line) {
